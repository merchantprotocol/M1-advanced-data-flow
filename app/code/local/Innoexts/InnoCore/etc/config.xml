<config>
    <modules>
        <Innoexts_InnoCore>
            <version>1.0.2</version>
        </Innoexts_InnoCore>
    </modules>
    <adminhtml>
        <translate>
            <modules>
                <Innoexts_InnoCore>
                    <files>
                        <default>Innoexts_InnoCore.csv</default>
                    </files>
                </Innoexts_InnoCore>
            </modules>
        </translate>
    </adminhtml>
    <global>
        <models>
            <innocore>
                <class>Innoexts_InnoCore_Model</class>
                <resourceModel>innocore_mysql4</resourceModel>
            </innocore>
            <innocore_mysql4>
                <class>Innoexts_InnoCore_Model_Mysql4</class>
            </innocore_mysql4>
        </models>
        <blocks>
            <innocore>
                <class>Innoexts_InnoCore_Block</class>
            </innocore>
        </blocks>
        <helpers>
            <innocore>
                <class>Innoexts_InnoCore_Helper</class>
            </innocore>
        </helpers>
    </global>
</config>