<?php
$groupId = $this->getGroupId ();
if ($this->helper('menupro')->isDevelopMode()) {
	echo "<div class='links'>";
    if ($this->title) {
        echo '<div class="block-title">';
        echo '<strong><span>' . $this->title . '</span></strong>';
        echo '</div>';
    }
	echo $this->getMenuHtml($groupId);
	echo "</div>";
} else {
	echo $this->getStaticHtml($groupId);
}